[config]
# Optional: Performance settings  
max_tokens_limit="128000"
# Minimum tokens limit for requests (to avoid errors with thinking model)
min_tokens_limit="40000"
request_timeout="90"
max_retries="2"
port=8086
anthropic_api_key=""
log_level="INFO"
db_file="proxy.db"
host="127.0.0.1"

# default model
big_model="claude_opus4"
middle_model="claude_sonnet4"
small_model="claude35_haiku"

[[provider]]
name = "demo"
base_url = "https://demo.ai/api/openai/v1"
api_key="sk-your-api-key"
big_models=["claude_opus4","claude3_opus"]
middle_models=["claude_sonnet4","claude37_sonnet"]
small_models=["claude35_haiku","claude3_haiku"]

[[provider]]
name = "DeepSeek"
base_url = "https://api.deepseek.com/v1"
api_key="your-deepseek-api-key"
big_models=["deepseek-coder"]
middle_models=["deepseek-chat"]
small_models=["deepseek-chat"]

[[provider]]
name = "OpenRouter"
base_url = "https://openrouter.ai/api/v1"
api_key="your-openrouter-api-key"
web_search = "openrouter.online"  # Use OpenRouter's built-in web search
big_models=["anthropic/claude-3-opus-20240229", "openai/gpt-4o"]
middle_models=["anthropic/claude-3-sonnet-20240229"]
small_models=["anthropic/claude-3-haiku-20240307"]

[[provider]]
name = "Bocha-Direct"
base_url = ""  # Not used for direct web search
web_search = "bocha.websearch"  # Use Bocha as direct web search provider
# Note: This provider bypasses LLM and only provides web search results
big_models=[]  # Not used
middle_models=[]  # Not used
small_models=[]  # Not used

[[provider]]
name = "Bocha-Plus-LLM"
base_url = "https://api.openai.com/v1"
api_key="your-openai-api-key"
web_search = "bocha.websearch"  # Use Bocha for search, OpenAI for LLM
big_models=["gpt-4o"]
middle_models=["gpt-4o"]
small_models=["gpt-4o-mini"]

# Transformer configuration
[transformers]
[transformers.deepseek]
enabled = true
providers = ["deepseek"]
models = ["deepseek-*"]
max_output = 8000

[transformers.openrouter]
enabled = true
providers = ["openrouter"]
models = ["*"]
cache_control = false

# Web Search Provider Configuration
[web_search_providers.bocha.websearch]
api_key = "your-bocha-api-token"  # or use env_key below
env_key = "BOCHA_API_TOKEN"  # Load API token from environment variable (recommended)
base_url = "https://api.bochaai.com/v1"  # Optional: override default base URL
